import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner kiritish = new Scanner(System.in);

       int a,sum=0;

        System.out.print("Enter number: ");
        a = kiritish.nextInt();
for( int i=1 ; i<=a; i++){
    sum=sum+(i*i);


}
        System.out.println("Sum of numbers: " + sum );
    }
}
